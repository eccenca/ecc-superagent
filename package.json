  "description": "Extended version of superagent.js, pimped with RX, better Link header parsing and HTTP Problem support",
  "main": "es5/component.js",
  "es5": "es5/component.js",
  "es6": "index.js",
  "scripts": {
    "test": "gulp test",
    "cover": "gulp cover",
