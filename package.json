  "license": "MIT",
  "devDependencies": {
    "@eccenca/dotfiles": "^2.3.1",
    "@eccenca/gulp-tasks": "^3.15.0",
    "ecc-test-helpers": "^15.2.0",
    "fs-extra": "^5.0.0",
    "globby": "^7.1.1",
    "gulp": "^3.9.0",
    "nock": "^9.1.6"
  },
  "dependencies": {
    "lodash": "^4.13.1",
    "methods": "^1.1.2",
